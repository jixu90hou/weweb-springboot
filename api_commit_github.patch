+			String headName = iterator.next();
+			System.out.println(headName + ":" + headers.get(headName) + "[\\r\\n]");
+		}
+		System.out.println("[\\r\\n]");
+		String responseStr = resourceName + "[" + description + "]";
+		return responseStr;
+	}
+
