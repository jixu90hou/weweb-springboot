+		strBuilder.append("HTTP/1.1[\\r\\n]");
+		System.out.println(strBuilder.toString());
+		MultivaluedMap<String, String> headers = httpHeader.getRequestHeaders();
+		Iterator<String> iterator = headers.keySet().iterator();
